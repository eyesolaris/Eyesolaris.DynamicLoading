<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Eyesolaris's Module Loader Library</Title>
    <Authors>Eyesolaris</Authors>
    <Description>Library for loading dynamic modules</Description>
	<Version>1.1</Version>
	<AssemblyVersion>1.1</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Eyesolaris.Logging" Version="1.0.0" />
    <PackageReference Include="Eyesolaris.ModuleBuildingBlocks" Version="1.1.0" />
    <PackageReference Include="Eyesolaris.Utilities" Version="1.0.0" />
  </ItemGroup>

</Project>
